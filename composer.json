{
    "name": "arikislam/replicate-php-client",
    "description": "A PHP client for the Replicate API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Arikislam\\ReplicatePhpClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "S M Ariq Islam",
            "email": "arikislam321@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Arikislam\\ReplicatePhpClient\\Providers\\ReplicatePhpClientServiceProvider"
            ],
            "aliases": {
                "Replicate": "Arikislam\\ReplicatePhpClient\\Facades\\Replicate"
            }
        }
    }
}
